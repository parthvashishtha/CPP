    #include <iostream>
    using namespace std;
    int main()
    {
        int Myarr[3][3];
        int Myarr1[3][3] = {1,3,2,4,2,6,5,1,1};
        int Myarr2[3][3] = {{2,4,5},{1,2,3,},{6,9,8}};

        for (int  i = 0; i < 3; i++)
        {
            for (int  j = 0; j< 3; j++)
            {
               cout<<Myarr2[i][j]<<"  ";
            }

            cout<<endl; 
            
        }

        cout<<endl<<"Enter values for 3*3 matrix"<<endl;
        for (int  i = 0; i < 3; i++)
        {
            for (int  j = 0; j< 3; j++)
            {
               cin>>Myarr1[i][j];
            }

             
            
        }

        cout<<endl<<"Values of 3*3 matrix"<<endl;

        for (int  i = 0; i < 3; i++)
        {
            for (int  j = 0; j< 3; j++)
            {
               cout<<Myarr1[i][j]<<" ";
            }

            cout<<endl; 
            
        }
        

    return 0;
    }
