
    #include <iostream>
    using namespace std;

    int add(int a, int b)
    {
        cout<<"Function 1() called"<<endl;
        return (a+b);
    }
    double add(double a, double b)
    {
        cout<<"Function 2() called"<<endl;
        return (a+b);
    }
    double add(int a, double b)
    {
        cout<<"Function 3() called"<<endl;
        return (a+b);
    }
    double add(double a, int b)
    {
        cout<<"Function 4() called"<<endl;
        return (a+b);
    }
    int main()
    {

        cout<<"Addition of numbers is "<<add(3,4)<<endl;
        cout<<"Addition of numbers is "<<add(3.5,4.5)<<endl;
        cout<<"Addition of numbers is "<<add(3,4.6)<<endl;
        cout<<"Addition of numbers is "<<add(3.8,4)<<endl;


    return 0;
    }
